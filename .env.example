# Flight Planner Environment Configuration
# Copy to .env and fill in values

# Cloudflare Worker URL (after deploying)
VITE_WORKER_URL=http://localhost:8787

# Weather API (metar-taf.com) - get key from https://metar-taf.com/api
VITE_WEATHER_API_KEY=
VITE_WEATHER_API_URL=https://metar-taf.com/api

# OpenAIP API - get key from https://www.openaip.net
VITE_OPENAIP_CLIENT_ID=
